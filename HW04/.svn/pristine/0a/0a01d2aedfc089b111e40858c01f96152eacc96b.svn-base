        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kxJzUpwMEeecHN65IlRRYA" name="updateState" specification="_kuMl8JwMEeecHN65IlRRYA">
          <ownedParameter xmi:id="_kxJzU5wMEeecHN65IlRRYA" name="ball" type="_kngIEJwMEeecHN65IlRRYA"/>
          <language>java</language>
          <body>cnt--;
			if (cnt == 0) {
				int newX = Randomizer.singleton.randomInt(0, 30);
				int newY = Randomizer.singleton.randomInt(0, 30);
				ball.setVelocity(new Point(newX, newY));
		cnt = 10;
		}
	</body>
