      <packagedElement xmi:type="uml:Class" xmi:id="_kpJt4JwMEeecHN65IlRRYA" name="CurveStrategy">
        <ownedComment xmi:id="_3fiTMJ27Eee2Y75hWZowQQ">
          <body>a strategy of changing the ball's velocity direction
@author Ye Wang
@author Dai Li
 *</body>
        </ownedComment>
        <generalization xmi:id="_kuL-4pwMEeecHN65IlRRYA" general="_kpV7IJwMEeecHN65IlRRYA"/>
        <ownedAttribute xmi:id="_kwH4kJwMEeecHN65IlRRYA" name="radius" visibility="package">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
        </ownedAttribute>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kxJMQpwMEeecHN65IlRRYA" name="CurveStrategy" specification="_kpLjEJwMEeecHN65IlRRYA">
          <ownedParameter xmi:id="_kxJMQ5wMEeecHN65IlRRYA" type="_kpJt4JwMEeecHN65IlRRYA" direction="return">
            <name xsi:nil="true"/>
          </ownedParameter>
          <language>java</language>
          <body>radius = Randomizer.singleton.randomDouble(0.2, 1);
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kxJzUJwMEeecHN65IlRRYA" name="updateState" specification="_kuKJsJwMEeecHN65IlRRYA">
          <ownedParameter xmi:id="_kxJzUZwMEeecHN65IlRRYA" name="ball" type="_kngIEJwMEeecHN65IlRRYA"/>
          <language>java</language>
          <body>Point oldV = ball.getVelocity();
		int oldX = oldV.x;
		int oldY = oldV.y;
		double newX = Math.round(oldX * Math.cos(radius) - oldY * Math.sin(radius));
		double newY = Math.round(oldY * Math.cos(radius) + oldX * Math.sin(radius));
		
		ball.setVelocity(new Point((int)newX, (int)newY));

	</body>
        </ownedBehavior>
