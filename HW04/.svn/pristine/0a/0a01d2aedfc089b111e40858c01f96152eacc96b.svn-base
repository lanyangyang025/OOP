        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kxHXEJwMEeecHN65IlRRYA" name="loadStrategy" specification="_kotB8JwMEeecHN65IlRRYA">
          <ownedParameter xmi:id="_kxHXEZwMEeecHN65IlRRYA" name="strategyName">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
