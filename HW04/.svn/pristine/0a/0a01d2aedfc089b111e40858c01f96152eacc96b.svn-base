        <ownedComment xmi:id="_3fhsIJ27Eee2Y75hWZowQQ">
          <body>a strategy of changing one of the ball's x and y velocity
@author Ye Wang
@author Dai Li
 *</body>
        </ownedComment>
        <generalization xmi:id="_kuXlEJwMEeecHN65IlRRYA" general="_kpV7IJwMEeecHN65IlRRYA"/>
        <ownedAttribute xmi:id="_kwNYIJwMEeecHN65IlRRYA" name="cnt" visibility="package">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          <defaultValue xmi:type="uml:LiteralString" xmi:id="_kwN_MJwMEeecHN65IlRRYA" value="5"/>
        </ownedAttribute>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kxLogJwMEeecHN65IlRRYA" name="updateState" specification="_kuVv4JwMEeecHN65IlRRYA">
          <ownedParameter xmi:id="_kxLogZwMEeecHN65IlRRYA" name="ball" type="_kngIEJwMEeecHN65IlRRYA"/>
          <language>java</language>
          <body>cnt--;
		if (cnt == 0) {
			Point oldV = ball.getVelocity();
			if (Math.abs(oldV.x) > Math.abs(oldV.y)){
				ball.setVelocity(new Point(-oldV.x, oldV.y));
			} else {
				ball.setVelocity(new Point(oldV.x, -oldV.y));
			}
			cnt = 5;
		}
	</body>
        </ownedBehavior>
