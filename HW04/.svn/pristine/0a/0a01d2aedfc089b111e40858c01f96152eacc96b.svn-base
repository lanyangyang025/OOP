        <ownedComment xmi:id="_3fhsIp27Eee2Y75hWZowQQ">
          <body>a strategy of changing the ball's radius
@author Ye Wang
@author Dai Li
 *</body>
        </ownedComment>
        <generalization xmi:id="_kuHGYZwMEeecHN65IlRRYA" general="_kpV7IJwMEeecHN65IlRRYA"/>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kxIlMZwMEeecHN65IlRRYA" name="updateState" specification="_kuDcAJwMEeecHN65IlRRYA">
          <ownedParameter xmi:id="_kxIlMpwMEeecHN65IlRRYA" name="ball" type="_kngIEJwMEeecHN65IlRRYA"/>
          <language>java</language>
          <body>int oldRad = ball.getRadius();
		int newRad = Randomizer.singleton.randomInt(Math.max(oldRad - 10, 1), Math.min(oldRad + 10, 30));
		ball.setRadius(newRad);
		Point oldLoc = ball.getLoc();
		ball.setLoc(new Point(oldLoc.x + (oldRad - newRad), oldLoc.y + (oldRad - newRad)));
	</body>
        </ownedBehavior>
