package yl128_pg23.game.server.model;

import java.io.Serializable;

/**
 * 
 * @author Yiqing Lu
 *
 */

public class Team implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 3576661829156152627L;
	String name;

	public Team(String name) {
		this.name = name;
	}

	public String toString() {
		return this.name;
	}

	public boolean equals(Team t1) {
		return t1.name.equals(this.name);
	}
}
