        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_kRAEk5sEEee9P6i_6jyonw" name="bounce" specification="_kQa1wpsEEee9P6i_6jyonw">
          <language>java</language>
          <body>Point border = canvas_getSize();
		if (origin.x + radius >= border.x) {
			velocity.x = -velocity.x;
			origin.x = 2 * border.x - 2 * radius - origin.x;
		}
		if (origin.y + radius >= border.y) {
			velocity.y = -velocity.y;
			origin.y = 2 * border.y - 2 * radius - origin.y;
		}
		if (origin.x - radius &lt;= 0) {
			velocity.x = -velocity.x;
			origin.x = 2 * radius - origin.x;
		}
		if (origin.y - radius &lt;= 0) {
			velocity.y = -velocity.y;
			origin.y = 2 * radius - origin.y;
		}
	</body>
        </ownedBehavior>
